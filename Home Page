<script>
  function validateSearch() {
    const input = document.getElementById('searchInput').value.trim().toLowerCase();

    if (input === "") {
      alert("Please enter a search term.");
      return false;
    }

    if (input.includes("gents")) {
      window.location.href = "For-Men.html";
      return false;
    }

    if (input.includes("ladies")) {
      window.location.href = "For-Women.html";
      return false;
    }
    
    if (input.includes("brands")) {
      window.location.href = "brands.html";
      return false;
    }

    alert("No matching page found for your search.");
    return false;
  }
</script>

</html>
